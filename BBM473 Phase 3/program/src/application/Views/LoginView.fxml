<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" minHeight="288.0" minWidth="366.0" prefHeight="288.0" prefWidth="437.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controllers.LoginController">
    <children>
        <Label fx:id="connStatusLabel">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="35.0" />
            </VBox.margin>
        </Label>
      <TextField fx:id="dburlField" alignment="CENTER" maxWidth="299.0" prefHeight="25.0" prefWidth="299.0" promptText="Database URL" text="jdbc:oracle:thin:@localhost:1521:dbs">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </TextField>
        <TextField fx:id="usernameField" alignment="CENTER" maxWidth="299.0" prefHeight="25.0" prefWidth="299.0" promptText="Username">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </TextField>
        <PasswordField fx:id="passwordField" alignment="CENTER" maxWidth="299.0" prefHeight="25.0" prefWidth="299.0" promptText="Password">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </PasswordField>
        <Button alignment="CENTER" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="92.0" text="Login">
            <VBox.margin>
                <Insets bottom="50.0" left="10.0" right="10.0" top="30.0" />
            </VBox.margin>
        </Button>
    </children>
</VBox>
